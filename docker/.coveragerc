[run]
source = .
omit =
    tests/*
    test_*.py
    *test*.py
    venv/*
    .venv/*
    */site-packages/*
    __pycache__/*
    build.sh
    debug_search.py
    demo_server.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
